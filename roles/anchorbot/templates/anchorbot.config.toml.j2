server = "{{ anchorbot_server }}"
token = "{{ anchorbot_access_token }}"
userId = "{{ anchorbot_user_id }}"

[rooms]
anchor = "{{ anchorbot_anchor_room_id }}"
announcements = "{{ anchorbot_announcements_room_id }}"
curators = "{{ anchorbot_curators_room_id }}"

[alias.woke]
title = "woke.net"
kind = "live"
dash = "https://live.woke.net/dash.mpd"
hls = "https://live.woke.net/hls.m3u8"

[alias.get]
title = "woke.net second stream"
kind = "live"
dash = "https://live2.woke.net/dash.mpd"
hls = "https://live2.woke.net/hls.m3u8"
